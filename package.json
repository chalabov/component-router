{
  "name": "component-router",
  "version": "2.0.0",
  "description": "Flux-based component routing solution",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:lib && npm run build:example",
    "build:clean": "rm -rf lib example",
    "build:lib": "babel -sd lib src",
    "build:example": "webpack",
    "prepublish": "npm run build",
    "start": "NODE_ENV=development webpack-dev-server",
    "lint": "eslint .",
    "karma": "karma start ./karma.conf.js",
    "test": "npm run karma --  --single-run --reporters nyan",
    "test:dev": "npm run karma -- --no-single-run --auto-watch --reporters nyan",
    "test:cov": "COVERAGE=1 npm run karma -- --single-run --reporters coverage",
    "check": "npm run lint && npm run test && npm run build",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/in-flux/component-router.git"
  },
  "keywords": [
    "component",
    "react-component",
    "flux-component",
    "react",
    "component router",
    "router",
    "partial router",
    "url-router",
    "url-state"
  ],
  "author": "Nik Butenko <nik@butenko.me> (http://butenko.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/in-flux/component-router/issues"
  },
  "homepage": "https://github.com/in-flux/component-router",
  "peerDependencies": {
    "react": "^0.13.3"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "flux": "^2.0.3",
    "flux-common-store": "^1.1.0",
    "lodash.isfunction": "^3.0.6",
    "lodash.isnull": "^3.0.0",
    "lodash.isundefined": "^3.0.1"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "babel-core": "^5.7.4",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.3.2",
    "babel-plugin-object-assign": "^1.2.0",
    "coveralls": "^2.11.3",
    "css-loader": "^0.15.5",
    "eslint": "^0.24.1",
    "eslint-loader": "^0.14.2",
    "eslint-plugin-react": "^2.7.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.0",
    "imports-loader": "^0.6.4",
    "inject-loader": "^2.0.0",
    "isparta-loader": "^0.2.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.12.37",
    "karma-coverage": "^0.4.2",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.2",
    "karma-nyan-reporter": "0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-webpack": "^1.6.0",
    "node-libs-browser": "^0.5.2",
    "phantomjs": "^1.9.17",
    "react-hot-loader": "^1.2.8",
    "react-motion": "^0.1.0",
    "react-swap": "^1.0.5",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  }
}
